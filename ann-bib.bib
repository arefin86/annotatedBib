	year = {2000},
	keywords = {ethnography, field research methods, qualitative methods, user studies},
	pages = {280--286},
	file = {ACM Full Text PDF:files/317/Millen - 2000 - Rapid Ethnography Time Deepening Strategies for H.pdf:application/pdf}
}

